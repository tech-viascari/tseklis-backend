{
  "name": "tseklis-backend",
  "version": "1.0.0",
  "description": "A backend for the Tseklis project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --inspect --env-file .env ./api/index.js",
    "host": "nodemon --inspect --env-file .env ./api/index.js --host",
    "start": "node api/index.js",
    "migrate": "env $(cat .env) knex migrate:latest --knexfile ./api/database/knexfile.cjs",
    "rollback": "env $(cat .env) knex migrate:rollback --knexfile ./api/database/knexfile.cjs",
    "create-migration": "knex migrate:make $npm_config_name --migrations-directory api/database/migrations"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "pg": "^8.13.1"
  }
}
